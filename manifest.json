{
  "manifest_version": 2,
  "name": "My Text Inserter",
  "version": "1.0",
  "description": "Inserts text into text fields",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contentScript.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": ["activeTab", "https://*/*", "http://*/*"]
  }


